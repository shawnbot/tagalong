{
  "name": "tagalong",
  "version": "0.3.0",
  "description": "HTML templating done right",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "bundle": "browserify src/bundle.js > tagalong.js",
    "minify": "uglifyjs tagalong.js > tagalong.min.js",
    "build": "npm run bundle && npm run minify",
    "watch": "watchy -w src,package.json -- npm run build"
  },
  "author": {
    "name": "Shawn Allen",
    "url": "https://github.com/shawnbot"
  },
  "repository": "shawnbot/tagalong",
  "license": "CC0",
  "dependencies": {
    "dotmap": "^0.1.0",
    "incremental-dom": "^0.1.0"
  },
  "devDependencies": {
    "browserify": "^10.2.6",
    "karma": "^0.12.37",
    "karma-browserify": "^4.2.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-source-map-support": "^1.0.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.17",
    "uglifyjs": "^2.4.10",
    "watchy": "^0.6.2"
  }
}
